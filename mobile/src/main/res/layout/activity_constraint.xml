<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_constraint"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="gdg.iojkuat.Constraint">

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button"
        app:layout_constraintLeft_toLeftOf="@+id/button2"
        android:layout_marginLeft="120dp"
        app:layout_constraintTop_toTopOf="@+id/activity_constraint"
        app:layout_constraintBottom_toBottomOf="@+id/activity_constraint" />

    <Button
        android:text="Button"
        android:layout_width="89dp"
        android:layout_height="49dp"
        android:id="@+id/button2"
        app:layout_constraintLeft_toLeftOf="@+id/activity_constraint"
        app:layout_constraintRight_toRightOf="@+id/activity_constraint"
        app:layout_constraintBottom_toTopOf="@+id/button"
        android:layout_marginBottom="152dp"
        app:layout_constraintHorizontal_bias="0.25" />
</android.support.constraint.ConstraintLayout>
